<h1>Genres#index</h1>

<p>ジャンル・データの一覧</p>
<table>
<tr>
  <th>Id</th><th>name</th><th>memo</th>
</tr>

<% @genres.each do |genre| %>
<tr>
  <td><%= genre.id %></td>
  <td><%= genre.name %></td>
  <td><%= genre.memo %></td>
  <td><%= link_to("Edit", "/genres/#{genre.id}") %></td>
</tr>
<% end %>
</table>
<p><%= link_to("ジャンル新規作成", "/genres/add" ) %></p>
